# OpenAI API 配置
OPENAI_API_KEY=your_api_key_here

# 默认对话模型
MODEL_NAME=gpt-3.5-turbo

# 视觉理解模型（用于图片分析）
# 推荐使用 gpt-4o、gpt-4-turbo 或 gpt-4-vision-preview
VISION_MODEL=gpt-4o

# 对话配置
MAX_TOKENS=1000
TEMPERATURE=0.8

# 服务器配置
HOST=0.0.0.0
PORT=8000

# CORS 配置
CORS_ORIGINS=http://localhost:5173,http://localhost:3000
